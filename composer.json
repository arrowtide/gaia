{
    "name": "arrowtide/gaia",
    "description": "The rapid development starterkit for your next headless Shopify store",
    "license": "Arrowtide Ltd. See https://github.com/arrowtide/gaia/blob/main/LICENCE.md for licence information.",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Arrowtide\\Gaia\\": "src"
        }
    },
    "require": {
        "statamic-rad-pack/shopify": "^5.0",
        "statamic/cms": "^5.0",
        "jonassiewertsen/statamic-livewire": "^3.7.0",
        "marcorieser/tailwind-merge-statamic": "^1.1.0",
        "arrowtide/statamic-payment-icons": "^1.0.0",
        "stillat/antlers-components": "^2.5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "statamic": {
            "name": "Gaia",
            "description": "The rapid development starterkit for your next headless Shopify store"
        },
        "laravel": {
            "providers": [
                "Arrowtide\\Gaia\\ServiceProvider"
            ]
        }
    }
}
